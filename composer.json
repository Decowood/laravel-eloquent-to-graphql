{
    "name": "decowood/laravel-eloquent-to-graphql",
    "description": "Automatically turn Eloquent models into GraphQL",
    "type": "library",
    "require": {
        "illuminate/support": ">=9.1",
        "doctrine/dbal": "^3.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "decowood",
            "email": "it@decowood.es"
        }
    ],
    "autoload": {
        "psr-4": {
            "Brausepulver\\EloquentToGraphQL\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brausepulver\\EloquentToGraphQL\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^8.21",
        "ext-pdo_sqlite": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Brausepulver\\EloquentToGraphQL\\Tests\\": "tests"
        }
    },
    "suggest": {
        "nuwave/lighthouse": "Uses the generated directives"
    }
}